.
TerraCore:Sequencing
  a owl:Class ;
  rdfs:label "Sequencing" ;
  rdfs:subClassOf TerraCore:Activity ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty TerraCore:usedSample ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty prov:generated ;
      owl:someValuesFrom TerraCore:SequenceFile ;
    ] ;

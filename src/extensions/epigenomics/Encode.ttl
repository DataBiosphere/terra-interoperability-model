    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty Encode:hasHealthStatus ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;

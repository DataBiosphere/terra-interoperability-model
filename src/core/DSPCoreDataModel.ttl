DSPCore:hasAgeCategory
  a owl:DatatypeProperty ;
  rdfs:domain DSPCore:Age ;
  rdfs:label "hasAgeCategory" ;
  rdfs:range [
      a rdfs:Datatype ;
      owl:oneOf (
          "Embryonic"
          "Postnatal"
        ) ;
    ] ;

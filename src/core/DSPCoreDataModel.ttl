  skos:prefLabel "VariantCallSet" ;
.
DSPCore:VariantCalling
  a owl:Class ;
  rdfs:label "VariantCalling" ;
  rdfs:subClassOf DSPCore:Activity ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty prov:generated ;
      owl:someValuesFrom DSPCore:VariantCallSet ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty prov:used ;
      owl:someValuesFrom DSPCore:AlignmentFile ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty prov:used ;

.
DSPCore:donatedBy
  a owl:ObjectProperty ;
  rdfs:domain DSPCore:BioSample ;
  rdfs:label "donatedBy" ;
  rdfs:range DSPCore:Donor ;
  owl:inverseOf DSPCore:donated ;
  skos:prefLabel "donatedBy" ;
.

DSPCore:File
  a owl:Class ;
  rdfs:comment "Used definition from http://semanticscience.org/resource/SIO_000396 but equivalence to http://www.w3.org/ns/dcat#Distribution doesn't make sense to me; thus we have our own object.  Also added electronic.  This will be subclassed for different file types to allow for file type-specific metadata, but for now, all metadata is an option for all Files." ;
  rdfs:label "File" ;
  rdfs:subClassOf obo:IAO_0000030 ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality 1 ;
      owl:onProperty DSPCore:fileSize ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality 1 ;
      owl:onProperty DSPCore:format ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality 1 ;
      owl:onProperty DSPCore:hash ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality 1 ;
      owl:onProperty DSPCore:hasReferenceAssembly ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality 1 ;
      owl:onProperty DSPCore:hasSponsor ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality 1 ;
      owl:onProperty DSPCore:mappedPairedEnd ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality 1 ;
      owl:onProperty DSPCore:mappedReadLength ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality 1 ;
      owl:onProperty DSPCore:pairedEnd ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality 1 ;
      owl:onProperty DSPCore:percentAlignedReads ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality 1 ;
      owl:onProperty DSPCore:platform ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality 1 ;
      owl:onProperty DSPCore:readCount ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:maxCardinality 1 ;
      owl:onProperty DSPCore:readLength ;
    ] ;
  skos:definition "A file is an information-bearing electronic object that contains a physical embodiment of some information using a particular character encoding." ;
  skos:prefLabel "File" ;

  owl:equivalentClass [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty DSPCore:hasLocation ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty DSPCore:hasStartPosition ;
    ] ;
  owl:equivalentClass [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty DSPCore:hasStopPosition ;
    ] ;
